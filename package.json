{
  "name": "rodsqueubot",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node bot.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && yarn run start",
    "build": "cd client && yarn run build"
  },
  "devDependencies": {
    "concurrently": "^6.0.2",
    "nodemon": "^2.0.2"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@react-firebase/auth": "^0.2.10",
    "@react-firebase/database": "^0.3.11",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "firebase": "^8.4.3",
    "firebaseui": "^4.8.0",
    "http-errors": "~1.6.3",
    "if-env": "^1.0.4",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "serve": "^11.3.2",
    "tmi.js": "^1.8.1"
  }
}
